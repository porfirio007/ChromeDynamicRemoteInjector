{
    "name": "Chrome Extensions Injector",
    "description": "Esegue codice restituito da api dinamicamente in una lista di siti anch'essa restituita da api",
    "manifest_version": 3,
    "version": "0.0.0.5",
    "content_scripts": [
        {
            "world": "MAIN",
            "all_frames": true,
            "run_at": "document_idle",
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "injector.js"
            ]
        }
    ]
}
