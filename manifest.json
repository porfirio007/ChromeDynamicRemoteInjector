{
    "name": "Injector",
    "description": "Inject Your Custom Inline JavaScript And Script Src Link Into Any Website.",
    "manifest_version": 3,
    "version": "0.0.0.5",
    "content_scripts": [
        {
            "world": "MAIN",
            "all_frames": true,
            "run_at": "document_idle",
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "injector.js"
            ]
        }
    ]
}
